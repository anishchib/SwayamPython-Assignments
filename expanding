def expanding(l):
    d = 0
    test=False
    for i in range(len(l)-1):
        if abs(l[i+1]-l[i]) > d:
            d=abs(l[i+1]-l[i])
            test=True
        else:
            test=False
    return test
def sumsquare(l):
    sume=sumo=0
    ll=[]
    for i in l:
        if i%2 == 0:
          sume=sume + i**2
        else:
          sumo=sumo + i**2
    ll.append(sumo)
    ll.append(sume)
# print(sume,sumo)
    return ll
